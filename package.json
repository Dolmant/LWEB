{
  "name": "lweb",
  "main": "server.js",
  "version": "1.0.0",
  "description": "",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "buildJS": "babel src/js --out-dir src/gen",
    "buildCSS": "sass src/lweb/scss/style.scss:dist/styles.css --no-source-map",
    "buildHTML": "shadow-cljs compile prerender && node prerender",
    "buildApp": "shadow-cljs release app",
    "report": "shadow-cljs run shadow.cljs.build-report app report.html",
    "optimizeJS": "babel --no-babelrc --plugins minify-dead-code-elimination dist/main.js --out-file dist/main1.js",
    "build": "npm run buildJS && npm run buildCSS && npm run buildApp && npm run optimizeJS && npm run buildHTML",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.55",
    "@material-ui/core": "^1.0.0",
    "@material-ui/icons": "^1.0.0",
    "babel-core": "^6.26.3",
    "babel-plugin-closure-elimination": "^1.3.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "classnames": "^2.2.6",
    "invariant": "^2.2.4",
    "lazysizes": "^4.1.0",
    "node-window-polyfill": "^1.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-slick": "^0.14.5",
    "react-spinners": "^0.4.4",
    "react-stripe-checkout": "^2.6.3",
    "toastr": "^2.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-minify": "^0.4.3",
    "babel-plugin-minify-dead-code-elimination": "^0.4.3",
    "sass": "^1.13.0",
    "shadow-cljs": "^2.5.0"
  }
}
