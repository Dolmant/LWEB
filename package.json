{
    "name": "lweb",
    "main": "server.js",
    "version": "1.0.0",
    "description": "",
    "directories": {
        "lib": "lib"
    },
    "scripts": {
        "buildJS": "babel src/js --out-dir src/gen",
        "buildCSS": "sass src/lweb/scss/style.scss:dist/styles.css --no-source-map",
        "buildHTML": "shadow-cljs release prerender && node prerender",
        "buildP": "shadow-cljs release app",
        "watch": "shadow-cljs watch app",
        "watchP": "shadow-cljs watch prerender",
        "report": "shadow-cljs run shadow.cljs.build-report app report.html",
        "build": "npm run buildJS && npm run buildCSS && npm run buildP && npm run buildHTML",
        "test": "echo \"Error: no test specified\" && exit 1",
        "deploy": "sh deploy.sh"
    },
    "author": "",
    "license": "ISC",
